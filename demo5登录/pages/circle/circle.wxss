/* pages/circle/circle.wxss */

page {
  width: 100%;
  height: 100%;
  background-color: #fff;
}

.circle-box {
  text-align: center;
  margin-top: 60rpx;
}

.circle {
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
}

.draw_btn {
  line-height: 80rpx;
  width: 350rpx;
  position: absolute;
  top: 330rpx;
  right: 0;
  left: 0;
  margin: 0 auto;
  border-radius: 5px;
  color: white;
  background-color: #f40;
}
